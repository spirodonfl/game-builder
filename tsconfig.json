{
  "compilerOptions": {
    "alwaysStrict": true,
    "module": "commonjs",
    "target": "es5",
    "sourceMap": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
    "skipLibCheck": true,
    "strictNullChecks": true
  },
  "exclude": [
    "node_modules"
  ]
}